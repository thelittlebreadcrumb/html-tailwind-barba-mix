{
  "name": "tailwind-alpine-mix",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "watch:all": "npm-run-all --parallel eleventy:start mix:build",
    "mix:dev": "mix",
    "mix:prod": "mix --production",
    "mix:build": "mix watch",
    "mix:watch-poll": "mix watch -- --watch-options-poll=1000",
    "mix:hot": "mix watch --hot",
    "eleventy:start": "eleventy --serve",
    "eleventy:build": "eleventy"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "autoprefixer": "^10.3.7",
    "browser-sync": "^2.27.5",
    "browser-sync-webpack-plugin": "^2.3.0",
    "laravel-mix": "^6.0.34",
    "postcss": "^8.3.9",
    "postcss-focus-visible": "^6.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nested": "^5.0.6"
  },
  "dependencies": {
    "@barba/core": "^2.9.7",
    "@barba/css": "^2.1.15",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^2.2.17"
  }
}
